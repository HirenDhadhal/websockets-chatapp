generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String
}

model ChatMessages {
  id Int @id @default(autoincrement())
  ChatId Int
  text String
  timestamp Int
  userId Int
}

model ChatUserMapping {
  id Int @id @default(autoincrement())
  userEmail String
  chatId Int
}